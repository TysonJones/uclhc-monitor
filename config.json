{
    "metrics": [
        {
            "database name": "MetricTest", 
            "group by ClassAd fields": [
                "Owner"
            ], 
            "metric type": "COUNTER", 
            "measurement name": "num_idle", 
            "job status": [
                "HELD", 
                "IDLE THEN REMOVED", 
                "RUNNING OR RAN"
            ], 
            "aggregation operation": "LAST", 
            "description": "Number of idle jobs per user at the end of each time bin, which remain running now"
        }
    ], 
    "bin duration": 1, 
    "outbox data limit": 1000
}